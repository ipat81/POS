        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/IGSpreadsheet;component/Assets/Global.xaml" />
                <ResourceDictionary Source="/IGSpreadsheet;component/Assets/Themes.xaml" />
